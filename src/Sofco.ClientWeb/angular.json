{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gaps": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.png"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/animate.css/animate.min.css",
              "node_modules/select2/dist/css/select2.css",
              "node_modules/datatables.net-bs/css/dataTables.bootstrap.css",
              "node_modules/datatables.net-buttons-bs/css/buttons.bootstrap.css",
              "src/assets/styles/plugins/awesome-bootstrap-checkbox.css",
              "src/assets/styles/plugins/datepicker/datepicker.css",
              "src/styles.scss",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/sweetalert2/dist/sweetalert2.min.css",
              "node_modules/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery-ui-dist/jquery-ui.js",
              "node_modules/select2/dist/js/select2.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/metismenu/dist/metisMenu.js",
              "node_modules/jquery-sparkline/jquery.sparkline.js",
              "vendor/pace/pace.min.js",
              "node_modules/pdfmake/build/pdfmake.min.js",
              "src/assets/js/vfs_fonts.js",
              "src/assets/js/pdfjs/pdf.js",
              "src/assets/styles/plugins/datepicker/bootstrap-datepicker.js",
              "src/assets/js/common.js",
              "node_modules/datatables.net/js/jquery.dataTables.js",
              "node_modules/datatables.net-bs/js/dataTables.bootstrap.js",
              "node_modules/datatables.net-buttons/js/dataTables.buttons.js",
              "node_modules/datatables.net-buttons/js/buttons.print.js",
              "node_modules/datatables.net-buttons/js/buttons.colVis.js",
              "node_modules/datatables.net-buttons-bs/js/buttons.bootstrap.js",
              "node_modules/jszip/dist/jszip.min.js",
              "node_modules/datatables.net-buttons/js/buttons.html5.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/sweetalert2/dist/sweetalert2.min.js",
              "node_modules/fullcalendar/dist/fullcalendar.min.js",
              "node_modules/fullcalendar/dist/locale/es.js",
              "node_modules/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "azsof01wd-dev": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.azsof01wd-dev.ts"
                }
              ]
            },
            "azsof01wd-test": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.azsof01wd-test.ts"
                }
              ]
            },
            "azsof01wd-qa": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.azsof01wd-qa.ts"
                }
              ]
            },
            "azsof01wt": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.azsof01wt.ts"
                }
              ]
            },
            "azgap01wp": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.azgap01wp.ts"
                }
              ]
            }
          }
        }, 
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gaps:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gaps:build:production"
            },
            "azsof01wd-dev": {
              "browserTarget": "gaps:build:azsof01wd-dev"
            },
            "azsof01wd-test": {
              "browserTarget": "gaps:build:azsof01wd-test"
            },
            "azsof01wd-qa": {
              "browserTarget": "gaps:build:azsof01wd-qa"
            },
            "azsof01wt": {
              "browserTarget": "gaps:build:azsof01wt"
            },
            "azgap01wp": {
              "browserTarget": "gaps:build:azgap01wp"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gaps:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery-ui-dist/jquery-ui.js",
              "node_modules/select2/dist/js/select2.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/metismenu/dist/metisMenu.js",
              "node_modules/jquery-sparkline/jquery.sparkline.js",
              "vendor/pace/pace.min.js",
              "node_modules/pdfmake/build/pdfmake.min.js",
              "src/assets/js/vfs_fonts.js",
              "src/assets/js/pdfjs/pdf.js",
              "src/assets/styles/plugins/datepicker/bootstrap-datepicker.js",
              "src/assets/js/common.js",
              "node_modules/datatables.net/js/jquery.dataTables.js",
              "node_modules/datatables.net-bs/js/dataTables.bootstrap.js",
              "node_modules/datatables.net-buttons/js/dataTables.buttons.js",
              "node_modules/datatables.net-buttons/js/buttons.print.js",
              "node_modules/datatables.net-buttons/js/buttons.colVis.js",
              "node_modules/datatables.net-buttons-bs/js/buttons.bootstrap.js",
              "node_modules/jszip/dist/jszip.min.js",
              "node_modules/datatables.net-buttons/js/buttons.html5.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/sweetalert2/dist/sweetalert2.min.js",
              "node_modules/fullcalendar/dist/fullcalendar.min.js",
              "node_modules/fullcalendar/dist/locale/es.js",
              "node_modules/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/animate.css/animate.min.css",
              "node_modules/select2/dist/css/select2.css",
              "node_modules/datatables.net-bs/css/dataTables.bootstrap.css",
              "node_modules/datatables.net-buttons-bs/css/buttons.bootstrap.css",
              "src/assets/styles/plugins/awesome-bootstrap-checkbox.css",
              "src/assets/styles/plugins/datepicker/datepicker.css",
              "src/styles.scss",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/sweetalert2/dist/sweetalert2.min.css",
              "node_modules/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.png"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "gaps-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "gaps:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "gaps",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}